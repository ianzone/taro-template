{
  "name": "react-taro",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:alipay": "taro build --type alipay",
    "build:h5": "taro build --type h5",
    "dev:weapp": "taro build --type weapp --watch",
    "dev:alipay": "taro build --type alipay --watch",
    "dev:h5": "taro build --type h5 --watch",
		"lint": "biome check --fix src --reporter summary",
		"postinstall": "lefthook install",
		"commit": "git add -A && git-cz"
  },
  "browserslist": ["defaults and fully supports es6-module", "maintained node versions"],
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@tarojs/components": "^4.0.4",
    "@tarojs/helper": "^4.0.4",
    "@tarojs/plugin-framework-react": "^4.0.4",
    "@tarojs/plugin-platform-alipay": "^4.0.4",
    "@tarojs/plugin-platform-h5": "^4.0.4",
    "@tarojs/plugin-platform-weapp": "^4.0.4",
    "@tarojs/react": "^4.0.4",
    "@tarojs/runtime": "^4.0.4",
    "@tarojs/shared": "^4.0.4",
    "@tarojs/taro": "^4.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-react": "^7.24.7",
    "@biomejs/biome": "^1.8.3",
    "@tarojs/cli": "^4.0.4",
    "@tarojs/vite-runner": "^4.0.4",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-preset-taro": "^4.0.4",
    "git-cz": "^4.9.0",
    "lefthook": "^1.7.12",
    "postcss": "^8.4.41",
    "react-refresh": "^0.14.2",
    "terser": "^5.31.5",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@swc/core",
    "@tarojs/binding",
    "core-js",
    "core-js-pure",
    "esbuild",
    "lefthook",
    "swiper"
  ]
}
